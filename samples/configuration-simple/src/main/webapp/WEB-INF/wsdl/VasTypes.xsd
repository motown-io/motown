  <xs:simpleType name="Latitude">
    <xs:annotation>
      <xs:documentation>
        Latitude of position in decimal degrees.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:double">
      <xs:minInclusive value="-90" />
      <xs:maxInclusive value="90" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="Longitude">
    <xs:annotation>
      <xs:documentation>
        Longitude of position in decimal degrees.
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:double">
      <xs:minInclusive value="-180" />
      <xs:maxInclusive value="180" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="Wgs84Coordinates">
    <xs:annotation>
      <xs:documentation>
        GPS location coordinates.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="latitude" type="tns:Latitude" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Latitude of position in decimal degrees.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="longitude" type="tns:Longitude" minOccurs="1" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>
            Longitude of position in decimal degrees.
          </xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="OpeningPeriod">
    <xs:annotation>
      <xs:documentation>
        This defines an opening period of a charging point. Usually a charge point will have one or more opening periods per day.
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
